    "rrweb-player": "^1.0.0-alpha.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "vue-tsc": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "typescript": "^5.0.2",
